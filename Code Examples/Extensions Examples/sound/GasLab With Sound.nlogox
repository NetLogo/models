        <setup></setup>
        <update><![CDATA[if length pressure-history > 0
[
  ;; pressure plot is 'smoothed' by averaging 3 sequential values, only if switch is set this way
  ifelse avg-plot-pressure?
    [ plotxy ticks (mean last-n 3 pressure-history) ]
    [ plotxy ticks pressure ]
]]]></update>
      </pen>
    </plot>
    <monitor x="135" precision="0" y="125" height="45" fontSize="11" width="109" sizeVersion="0" display="pressure">pressure</monitor>
    <switch x="270" y="470" height="33" on="false" variable="collisions?" width="113" sizeVersion="0" display="collisions?"></switch>
    <switch x="270" y="435" height="33" on="false" variable="wall-hits?" width="112" sizeVersion="0" display="wall-hits?"></switch>
    <switch x="45" y="345" height="33" on="false" variable="model-time?" width="185" sizeVersion="0" display="model-time?"></switch>
    <switch x="45" y="380" height="33" on="false" variable="real-time?" width="185" sizeVersion="0" display="real-time?"></switch>
