    <switch x="565" y="495" height="33" on="true" variable="single-particle-collisions?" width="205" sizeVersion="0" display="single-particle-collisions?"></switch>
    <slider x="45" step="0.025" y="415" height="33" min="0.5" direction="Horizontal" max="5" default="1.325" variable="real-time-pacer" units="sec" width="185" sizeVersion="0" display="real-time-pacer"></slider>
