    <plot x="566" yMax="100.0" yAxis="pressure" y="10" xMin="0.0" height="145" legend="false" xMax="20.0" autoplot="true" yMin="0.0" width="203" sizeVersion="0" xAxis="time" display="Pressure vs. Time">
      <setup></setup>
