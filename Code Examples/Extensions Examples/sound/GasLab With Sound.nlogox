    <switch x="270" y="400" height="33" on="false" variable="pressure?" width="112" sizeVersion="0" display="pressure?"></switch>
    <note x="323" y="327" height="18" textColorLight="-16777216" fontSize="11" width="174">Listen to all the particles</note>
    <note x="63" y="321" height="18" textColorLight="-16777216" fontSize="11" width="138">Time in a model</note>
    <slider x="385" step="5" y="470" height="33" min="0" direction="Horizontal" max="100" default="30.0" variable="collisions-loudness" width="165" sizeVersion="0" display="collisions-loudness"></slider>
    <slider x="385" step="5" y="435" height="33" min="0" direction="Horizontal" max="100" default="30.0" variable="wall-hits-loudness" width="165" sizeVersion="0" display="wall-hits-loudness"></slider>
    <slider x="385" step="5" y="400" height="33" min="0" direction="Horizontal" max="120" default="60.0" variable="pressure-loudness" width="165" sizeVersion="0" display="pressure-loudness"></slider>
