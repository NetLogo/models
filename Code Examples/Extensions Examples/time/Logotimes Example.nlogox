<?xml version="1.0" encoding="utf-8"?>
<model version="NetLogo 7.0.0" snapToGrid="true">
  <code>extensions [ time ]

globals
[
  ; The first three globals are Logotime variables to track time in the model
  sim-time               ; The current simulation time
  sim-start-logotime     ; The time when simulation starts
  sim-end-logotime       ; The simulation end time

  ; The remaining globals are normal NetLogo variables that hold the current time in
  ; various units produced by the time extension. Their values are displayed in the
  ; monitors on the Interface.
  the-year
  the-month
  the-month-name
  the-day
  the-day-of-week
  the-day-of-year
  the-hour
  the-minute
  the-second

  time-simulated   ; Text string showing how much time has been simulated, with the units specified on the Interface
  time-remaining   ; Text string showing how much time remains to be simulated, with the units specified on the Interface
]

to setup

  clear-all
  reset-ticks

  ; First, set the start and end Logotime variables from the text input on the Interface
  set sim-start-logotime time:create-with-format sim-start-time "yyyy/M/d HH:mm:ss"  ; NEVER use "YYYY" instead of "yyyy"
  set sim-end-logotime   time:create-with-format sim-end-time   "yyyy/M/d HH:mm:ss"

  ; Make sim-time a Logotime that is automatically advanced by "ticks", with the time step defined
  ; by the time step units and length from the Interface. For example, if "time-step-units" in the Interface chooser
  ; is "hours" and the "time-step-length" slider is set to 4, then each tick represents 4 hours.
  ; Normally, the time step units and length are `hardwired` by using a statement like:
  ; set sim-time time:anchor-to-ticks sim-start-time 1 "day"
  set sim-time time:anchor-to-ticks sim-start-time time-step-length time-step-units

  ; Now set the values of the global time variables, for the simulation start time
  update-time-variables

end

to go

  tick

  ; Use the is-after? primitive to determine if it is time to stop
  if time:is-after? sim-time sim-end-time
  [
    print "End of simulation"
    stop
  ]

  ; Otherwise, update the time variables
  update-time-variables

  ; Output the current simulation time in a custom format
  print time:show sim-time "M/d/yyyy H:mm:ss"

end

to update-time-variables

  ; Use the time extension's get and show commands to report the current time in various units.
  ; time:get reports numbers, and time:show reports text strings.
  set the-year          time:get "year"        sim-time
  set the-month         time:get "month"       sim-time
  set the-month-name    time:show sim-time "MMMM"
  set the-day           time:get "day"         sim-time
  set the-day-of-week   time:get "dayofweek"   sim-time
  set the-day-of-year   time:get "dayofyear"   sim-time
  set the-hour          time:get "hour"        sim-time
  set the-minute        time:get "minute"      sim-time
  set the-second        time:get "second"      sim-time

  ; As an example use of primitives for time-based calculations, determine the time simulated
  ; so far and the time remaining, as the difference between current simulation time (sim-time) and the
  ; starting and ending times
  set time-simulated (word (time:difference-between sim-start-time sim-time time-step-units) " " time-step-units)
  set time-remaining (word (time:difference-between sim-time sim-end-time time-step-units) " " time-step-units)

end


