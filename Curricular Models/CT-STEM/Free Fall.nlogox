<model version="NetLogo 7.0.0-internal1" snapToGrid="true">
  <code><![CDATA[globals [
  gravitational-acceleration ; A number that represents the gravitational constant
  displacement ; the distance covered in a single step by the ball
  total-displacement ; the total displacement of the ball
  ball ; the ball that is shown free falling in the view
]

to setup
  clear-all
  create-turtles 1 ; create the ball, then initialize its variables
  [
    setxy 0 max-pycor * 0.98 ; this is the starting position of the ball
    set shape "circle"
    set color red
    set size 8
    set label mass-of-ball
    set ball self
    set heading 180
  ]
  ; now we set the gravitational constant that will cause the ball to free fall
  set gravitational-acceleration 9.8
  reset-ticks
  ; add some green patches at the bottom of the view to represent the Earth
  ask patches with [ pycor < 0 ] [ set pcolor green ]
end

; procedure to calculate the movement of the ball
to proceed-for-one-step

  ; calculate displacement of the ball in a second using Newton's equation: s = u * t + 1/2 * a * t^2
  ; since it is one step, t = 1.

  set displacement velocity + ( gravitational-acceleration / 2 )

  ; stop the model if the ball will be below the ground in the next step
  if ([ycor] of one-of turtles) - displacement < 0 [ stop ]
  ask ball
  [
    if ycor > (min-pycor + 1)
    [
      ; now leave a trace
      hatch 1
      [
        set size 3
        stamp
        die
      ]
      ; if a user changes mass-of-ball during the fall
      set label mass-of-ball

      ; now we let the ball free fall
      forward displacement
      set total-displacement ( total-displacement + displacement )
    ]
  ]
  tick
end

;;; Reporters ;;;
; Reporter to return the velocity of the ball using v = a*t
to-report velocity
  report gravitational-acceleration * ticks
end

; Reporter to return the kinetic energy of the ball using KE = (1/2)*m*v^2
to-report kinetic-energy
  report 1 / 2 * mass-of-ball * ( velocity ^ 2 )
end

; Reporter to return the current potential-energy of the ball using PE = m*g*h
to-report potential-energy
  let height [ycor] of one-of turtles
  report mass-of-ball * gravitational-acceleration * height
end

; Reporter to return the current total energy using KE + PE = TE
to-report total-energy
  report kinetic-energy + potential-energy
end


; Copyright 2020 Uri Wilensky.
; See Info tab for full copyright and license.]]></code>
