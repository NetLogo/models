      <display><![CDATA[Temperatures]]></display>
      <xAxis><![CDATA[colors]]></xAxis>
      <yAxis><![CDATA[quantity]]></yAxis>
      <setup><![CDATA[set-plot-y-range 0 count turtles
set-histogram-num-bars 1 + (length colors)

let bottom (round room-temp)
let top (round melting-temp)
let index 0
foreach colors [ c ->
  create-temporary-plot-pen (word bottom " - " top)
  set-plot-pen-mode 1
  set-plot-pen-color c
  set pens lput (word bottom " - " top) pens

  set index index + 1
  set bottom top
  set top (round ((index * temp-range) + melting-temp))
]]]></setup>
      <update/>
      <pens>
        <pen inLegend="false" color="-2674135" mode="bar" interval="1.0">
          <setup/>
          <update><![CDATA[if not histogram? [ stop ]
let index 0
foreach colors [ c ->
  set-current-plot-pen (item index pens)
  plot-pen-reset
  if any? turtles with [ color = c ] [
    plotxy index count turtles with [ color = c ]
  ]
  set index index + 1
]]]></update>
          <display><![CDATA[quantity]]></display>
        </pen>
      </pens>
    </plot>
