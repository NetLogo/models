  <widgets>
    <view x="275" maxPycor="20" updateMode="1" wrappingAllowedX="true" y="30" frameRate="30.0" minPycor="-20" height="337" showTickCounter="true" patchSize="8.0" fontSize="10" wrappingAllowedY="true" width="816" tickCounterLabel="ticks" maxPxcor="50" minPxcor="-50"></view>
    <button x="20" y="10" height="33" disableUntilTicks="false" forever="false" kind="Observer" width="65">setup</button>
    <button x="90" y="10" height="33" disableUntilTicks="true" forever="true" kind="Observer" width="65">go</button>
