      <setup><![CDATA[set-plot-y-range 0 count turtles
set-histogram-num-bars 1 + (length colors)

let bottom (round room-temp)
let top (round melting-temp)
let index 0
foreach colors [ c ->
  create-temporary-plot-pen (word bottom " - " top)
  set-plot-pen-mode 1
