  <code><![CDATA[; Globals declared in widgets
; num-turtles               ;; number of conic-parts turtles to create
; conic-constant            ;; constant used to define the conic section - the sum of the distances
                            ;;   from a point to the foci.

breed [ foci focus ]
breed [ conic-parts conic-part ]
conic-parts-own [ conic-dist ]  ;; sum of distances to foci

globals [
         selected-focus          ;; the focus selected for dragging - or "nobody"
         distance-turtle         ;; the conic-part for which "conic-distance" will be shown - or nobody
                                 ;; mouse-x and mouse-y are used to filter out redundant mouse down events
         new-distance-turtle?    ;; true if a new distance turtle is wanted, but at least one focus is still needed
         recent-mouse-x          ;; rounded x-cor of most recent mouse click
         recent-mouse-y          ;; rounded y-cor of most recent mouse click
         MOUSE-CLICK-CLOSENESS   ;; how close mouse click must be to focus for dragging
         CONIC-EPS               ;; how close conic-dist must be to conic-constant for a turtle to stop
         BACKGROUND-COLOR        ;; color of background
         CONIC-PART-COLOR        ;; color of conic-parts turtles that move to form conic section
         CONIC-PART-SHAPE        ;; shape of conic-parts turtles
         CONIC-PART-SIZE         ;; size of conic-parts turtles
         FOCUS-COLOR             ;; color of ellipse focus turtles
         FOCUS-SHAPE             ;; shape of focus turtles
         FOCUS-SIZE              ;; size of focus turtles
         DISTANCE-LINK-COLOR     ;;  color of distance links
         DISTANCE-LINK-THICKNESS ;;  thickness of distance links
]

to setup
  clear-all
  initialize-values
  color-background-patches
  create-conic-parts num-turtles [
    set color CONIC-PART-COLOR
    set shape CONIC-PART-SHAPE
    set size CONIC-PART-SIZE
    setxy random-xcor random-ycor
  ]

  ; there is no recent mouse-click, so make coordinates outside the world
  set recent-mouse-x max-pxcor + 1
  set recent-mouse-y max-pycor + 1

  ; no focus exists or is selected initially
  set selected-focus nobody

  ; no distance turtle exists initially
  set distance-turtle nobody
  set new-distance-turtle? false

  reset-ticks
end

to initialize-values
  set CONIC-EPS               0.04
  set BACKGROUND-COLOR        black
  set CONIC-PART-COLOR        green
  set CONIC-PART-SHAPE        "circle"
  set CONIC-PART-SIZE         1
  set FOCUS-COLOR             white
  set FOCUS-SHAPE             "circle"
  set FOCUS-SIZE              2
  set DISTANCE-LINK-COLOR     red
  set DISTANCE-LINK-THICKNESS 1.5
  set MOUSE-CLICK-CLOSENESS   3
end

to go
  ; On mouse click create a focus if there are not enough, or drag an existing focus
  ifelse mouse-down?
    [ create-or-move-focus-if-mouse-down ]
  ; otherwise, make sure any previously selected focus is deselected
  [
    set selected-focus nobody
  ]

  ; Move the conic-parts turtles.
  ask conic-parts [ move-conic-parts ]
  adjust-dist-links-labels
  tick
end

; The mouse is down. A new focus is created if needed
; Moves a conic focus if the mouse nearby (mouse dragging).
to create-or-move-focus-if-mouse-down
  ; Should only be called if mouse is down
  if mouse-down? [
    let mouse-x (round mouse-xcor)
    let mouse-y (round mouse-ycor)
    ; For each mouse click mouse-down? may remain true for multiple calls to this
    ; procedure,so we should ignore the subsequent calls until the mouse
    ; coordinates change
    if (mouse-x != recent-mouse-x) or  (mouse-y != recent-mouse-y) [
      set recent-mouse-x mouse-x
      set recent-mouse-y mouse-y

      ifelse need-a-focus?
        [ create-foci 1 [
          setxy mouse-xcor mouse-ycor
          set color FOCUS-COLOR
          set shape FOCUS-SHAPE
          set size FOCUS-SIZE
          ]
          if not need-a-focus? and new-distance-turtle?
          [ create-distance-turtle ]
      ]
      ; Move focus if it is near a mouse click
      [
        handle-select-and-drag
      ]
    ]
  ]
end

; If a focus has been selected move it to the mouse location.
to handle-select-and-drag
  ; if no focus is selected-focus
  ifelse selected-focus = nobody  [
    ; pick the closest focus
    set selected-focus min-one-of foci [distancexy mouse-xcor mouse-ycor]
    ; check whether or not it's close enough
    if [ distancexy mouse-xcor mouse-ycor ] of selected-focus > MOUSE-CLICK-CLOSENESS [
      set selected-focus nobody ; if not, don't select it
    ]
  ][
    ; if a focus is selected-focus, move it to the mouse
    ask selected-focus [ setxy mouse-xcor mouse-ycor ]
  ]
end

; Move the turtles that will form the conic.
; Move randomly if there is no focus, or turtle is getting further from distance
;  to conic.
; Stop if within CONIC-EPS of desired "distance".
; Move forward if distance to conic is decreasing.
to move-conic-parts ; turtle procedure
  ; move randomly if a focus is needed
  ifelse need-a-focus?
  [ rt random-float 360 turn-if-cant-move 1 fd 1]
  [ let old-conic-dist conic-dist
    set conic-dist compute-conic-dist
    ; Turtle stops if it is within CONIC-EPS of desired "distance"
    ; If the turtle is not reducing the "distance" make a random turn and move
    ; forward, otherwise just move forward
    if (abs (conic-constant - conic-dist) > CONIC-EPS) [
      if ((conic-dist = old-conic-dist) or
        ((conic-dist < old-conic-dist) and (conic-dist < conic-constant)) or
        ((conic-dist > old-conic-dist) and (conic-dist > conic-constant)))
      [ rt random-float 360 ]
      turn-if-cant-move 1
      fd 1
    ]
  ]
end

; Reverse direction if it is not possible to go forward (turtle is near an edge).
to turn-if-cant-move [ dist ]
  if not can-move? 1  [ rt 180 ]
end

; Set color of patches.
to color-background-patches
  ask patches [ set pcolor BACKGROUND-COLOR ]
end

; Report true if user must select at least one more focus.
to-report need-a-focus?
  report count foci < 2
end

; This is the sum of the distances from the conic-part turtle to the two foci of
; the ellipse.
to-report compute-conic-dist ; turtle procedure
  report (sum [ distance myself ] of foci)
end

; Create a distance turtle if don't need a focus.
; If need a focus, set variable to create distance-turtle later
to create-distance-turtle
  ifelse need-a-focus? [
    set new-distance-turtle? true ]
  [
    clear-any-distance-turtle

    ;; establish a random conic part as the distance turtle
    set distance-turtle one-of conic-parts
    ask distance-turtle [
      create-links-with foci
      ask my-links [
        set color DISTANCE-LINK-COLOR
        set thickness DISTANCE-LINK-THICKNESS ]
    ]
    adjust-dist-links-labels
  ]
end

; If a distance turtle exists, remove its links
; and set the distance-turtle variable to nobody
to clear-any-distance-turtle
  if distance-turtle != nobody [
    ; need to decommision a distance turtle
    ask distance-turtle [ ask my-links [ die ]]
    set distance-turtle nobody
    ; the turtle still exists as a conic part
  ]
  set new-distance-turtle? false
end


;;;; Procedures visualize the distance from a special distance-turtle to the foci
to-report explain-conic-distance
  let the-conic-dist [ precision conic-dist 1 ] of distance-turtle
  let focal-distances map [d -> [ precision (distance distance-turtle) 1 ] of d ] sort foci
  let value (word the-conic-dist " = " item 0 focal-distances " + " item 1 focal-distances " = sum of distances to foci")
  report value
end

; Adjust link distance labels if they exist
to adjust-dist-links-labels
  if distance-turtle != nobody [
    ask distance-turtle [ ask my-links [
      set label precision link-length 1]
    ]
  ]
end

; Reports true if ticks are being counted.
; Used here to determine if the startup procedure has been run
to-report ticks-started?
  let val false
  carefully [
    if ticks >= 0 [
      set val true ]]
  [ ]
  report val
end


